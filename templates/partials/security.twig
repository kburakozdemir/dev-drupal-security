<div class="table-responsive">
	<table class="table table-striped table-bordered table-sm caption-top">
		<caption>
			Security Updates
		</caption>
		<tr>
			<th>id</th>
			<th>server</th>
			<th>instancename</th>
			<th>module</th>
			<th>module_version</th>
			<th>servertime</th>
		</tr>
		{% for security in securityUpdates %}
			<tr>
				<td>{{ security.id }}</td>
				<td>{{ security.server }}</td>
				<td>{{ security.instancename }}</td>
				<td>{{ security.module }}</td>
				<td>{{ security.module_version }}</td>
				<td>{{ security.servertime }}</td>
			</tr>
		{% endfor %}
	</table>
</div>
